{
  "name": "QuickPinry",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Quickly pin images to Pinry with a right-click.",
  "browser_action": {
    "default_icon": "icon_48.png"
  },
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },  
  "permissions": [ "tabs", "storage", "contextMenus", "cookies", "http://*/*", "https://*/*" ],
  "background": { "scripts": ["main.js"] },
  "options_page": "options.html"
}
